@model IEnumerable<SecondHandStoreApp.Models.ApplicationUser>

@{
    ViewBag.Title = "ListUsers";
}

<h2>ListUsers</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EmailConfirmed)
        </th>    
        <th>
            @Html.DisplayNameFor(model => model.PhoneNumber)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.MyUser.City)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.MyUser.Address)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.MyUser.SellerID)
        </th>
           
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EmailConfirmed)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PhoneNumber)
        </td>

        <td>
            @Html.DisplayFor(modelItem => item.MyUser.City)
        </td>

        <td>
            @Html.DisplayFor(modelItem => item.MyUser.Address)
        </td>

        <td>
            @Html.DisplayFor(modelItem => item.MyUser.SellerID)
        </td>

        <td>
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id }) |
        </td>
    </tr>
}

</table>
